{
  "extends": ["plugin:@nx/react", "../.eslintrc.json"],
  "@typescript-eslint/quotes": [
    "error",
    "single",
    {
      "allowTemplateLiterals": true
    }
  ],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
